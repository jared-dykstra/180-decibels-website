{
  "name": "180-decibels-server",
  "repository": {
    "type": "github",
    "url": "https://github.com/jared-dykstra/180-decibels"
  },
  "version": "0.1.0",
  "engines": {
    "node": ">=10.9.0",
    "npm": ">=6.4.0",
    "yarn": ">=1.10.0"
  },
  "private": true,
  "dependencies": {
    "apollo-datasource": "^0.2.1",
    "apollo-server-express": "^2.2.3",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "config": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-tag.macro": "^2.0.0",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "pg": "^7.7.1",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-plugin-macros": "^2.4.2",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "sqlite3": "^4.0.4"
  },
  "scripts": {
    "start": "node build/main.js",
    "debug": "babel-node src/main.js",
    "build": "babel src -d build -s --copy-files",
    "db:create": "yarn run db:migrate && yarn run db:seed",
    "db:migrate": "yarn run knex migrate:latest",
    "db:seed": "yarn run knex seed:run",
    "knex": "knex",
    "dev": "nodemon --config nodemon.json ./src/main.js",
    "lint": "eslint --ignore-path ../.gitignore .",
    "lint:fix": "eslint --ignore-path ../.gitignore --fix .",
    "test": "jest .*test.*test.js --watchAll",
    "test:ci": "jest .*test.*test.js",
    "ci": "yarn run lint && yarn run test:ci"
  }
}
