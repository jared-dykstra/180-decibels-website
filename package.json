{
  "name": "180-decibels-server",
  "repository": {
    "type": "github",
    "url": "https://github.com/jared-dykstra/180-decibels"
  },
  "version": "0.1.0",
  "engines": {
    "node": ">=10.9.0",
    "yarn": ">=1.12.3"
  },
  "private": true,
  "dependencies": {
    "compression": "^1.7.3",
